{
    "name": "gemorroj/phpwhois-lite",
    "description": "PHPWhois-lite",
    "keywords": ["whois","phpwhois"],
    "type": "library",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Gemorroj"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-curl": "*",
        "ext-pcre": "*",
        "algo26-matthias/idna-convert": "^4.2",
        "psr/log": "^3.0.2",
        "psr/cache": "^3.0.0",
        "symfony/polyfill-mbstring": "^1.31",
        "symfony/polyfill-php84": "^1.31"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5.15",
        "friendsofphp/php-cs-fixer": "^3.75.0",
        "phpstan/phpstan": "^2.1.11",
        "symfony/console": "^7.2.5",
        "symfony/cache": "^7.2.5"
    },
    "autoload": {
        "psr-4": {
            "PHPWhoisLite\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPWhoisLite\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "ext-mbstring": "*",
        "php": ">=8.4"
    }
}
