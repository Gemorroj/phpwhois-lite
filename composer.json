{
    "name": "gemorroj/whordap",
    "description": "WHOIS/RDAP client",
    "keywords": ["whois","phpwhois", "rdap", "phprdap", "lookup"],
    "type": "library",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Gemorroj"
        }
    ],
    "require": {
        "php": ">=8.4",
        "ext-curl": "*",
        "ext-pcre": "*",
        "algo26-matthias/idna-convert": "^4.2",
        "psr/log": "^3.0.2",
        "psr/cache": "^3.0.0",
        "symfony/polyfill-mbstring": "^1.31"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.1.2",
        "friendsofphp/php-cs-fixer": "^3.75.0",
        "phpstan/phpstan": "^2.1.11",
        "symfony/console": "^7.2.5",
        "symfony/cache": "^7.2.5"
    },
    "autoload": {
        "psr-4": {
            "WhoRdap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WhoRdap\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "ext-mbstring": "*"
    }
}
